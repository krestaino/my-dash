{
  "name": "my-dash-ui",
  "version": "2.3.0",
  "license": "MIT",
  "author": "Kevin Restaino <kevinrestaino@gmail.com>",
  "description": "UI for My Dash",
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/styles.css -o public/styles.min.css -w",
    "build:css": "postcss src/styles.css -o public/styles.min.css --env production",
    "serve": "node src/server.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "tailwindcss": "^1.1.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "cssnano": "^4.1.10",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "serve": "^11.1.0",
    "serve-handler": "^6.1.0",
    "tailwindcss-dark-mode": "^1.0.3",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
